/*Settings Below this line*/const int ABSringteeth = 53; //How many teeth are on your ABS Tone ringsconst int maxSlip = 0.10; //Max wheel spin before we start sending an output signal./*DO NOT EDIT BELOW THIS LINE*/int max = maxSlip;int tc = ABSringteeth;int slip = 0;int c1 = 0;int c2 = 0;int c3 = 0;int c4 = 0;int frdiff = 0;int fv = 0;int rv = 0;int op = 0;bool error = false;bool bypass = false;int ec = 0;void setup() {    pinMode(3, INPUT);    pinMode(5, INPUT);    pinMode(6, INPUT);    pinMode(8, INPUT);    pinMode(9, INPUT);        pinMode(11, OUTPUT);}void loop() {  if (digitalRead(8) == HIGH) {    bypass = true;  }else{    bypass = false;  }  if (!bypass) {    if (!error) {      if (!digitalRead(3) == HIGH) {      c1 = pulseIn(3, HIGH); // Front Left      }else{        error = true;        ec = 5      }            if (!digitalRead(5) == HIGH) {      c2 = pulseIn(5, HIGH); // Front Right      }else{        error = true;        ec = 6      }            if (!digitalRead(6) == HIGH) {      c3 = pulseIn(6, HIGH); // Rear Left      }else{        error = true;        ec = 7      }            if (!digitalRead(7) == HIGH) {      c4 = pulseIn(9, HIGH); // Rear Right      }else{        error = true;        ec = 8      }            delay(250);      if (c1== 0 && c2 >= 1 && c3 >= 1 && c4 >= 1) {        error = true;        ec = 1;      }      if (c1>= 1 && c2 == 0 && c3 >= 1 && c4 >= 1) {        error = true;        ec = 2;      }      if (c1>= 1 && c2 >= 1 && c3 == 0 && c4 >= 1) {        error = true;        ec = 3;      }      if (c1>= 1 && c2 >= 1 && c3 >= 1 && c4 == 0) {        error = true;        ec = 4;      }            if (c1 == 0) {        return;      }else if (c2 == 0){        return;      }else if (c3 == 0) {        return;      }else if (c4 == 0) {        return;      }          fv = c1+c2;      rv = c3+c4;       frdiff = (fv)-(rv);      frdiff = frdiff/100;      op = 255*frdiff;            if (op > 255) {        op = 255;      }else if (op < 0) {        op = 0;      }          if (frdiff >= max) {        analogWrite(11, op);      }else {        digitalWrite(11, LOW);      }    }else{      if (ec == 1) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else      if (ec == 2) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 3) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 4) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 5) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 6) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 7) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }else if (ec == 8) {        pinMode(8, OUTPUT);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(500);        digitalWrite(8, HIGH);        delay(500);        digitalWrite(8, LOW);        delay(1500);        return;      }    }  }}